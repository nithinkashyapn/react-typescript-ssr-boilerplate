    "react-hot-loader": "^4.0.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.3",
    "ts-loader": "^3.5.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.9.1",
    "typescript": "^3.1.1",
    "url-loader": "^1.0.1",
    "webpack": "^3.11.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-dev-server": "^3.1.0",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-hot-server-middleware": "^0.4.0",
