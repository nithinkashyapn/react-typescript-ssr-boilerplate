{
  "name": "react-typescript-ssr",
  "version": "0.1.0",
  "main": "build/server/server.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "dev": "npm run clean && cross-env NODE_ENV=development npm run watch:server",
    "build": "npm run clean && npm run build:server && npm run build:client && npm run build:serverRenderer",
    "build:client": "webpack --config ./webpack.client.prod.ts -p",
    "build:serverRenderer": "webpack --config ./webpack.server.ts -p",
    "build:server": "tsc -p tsconfig-server.json",
    "watch:server": "nodemon",
    "start": "cross-env NODE_ENV=production node build/server/server.js"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^4.4.1",
